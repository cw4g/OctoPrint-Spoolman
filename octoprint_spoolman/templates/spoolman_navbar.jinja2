<!-- Modal-Dialog -->
<div id="navbar_spoolDialog" class="modal hide fade">
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
        <h3 class="modal-title">Selected spool</h3>
    </div>
    <div class="modal-body">
        <span>ID</span>: <strong data-bind="text: id"></strong><br>
        <span>Name</span>: <strong data-bind="text: name"></strong><br>
        <span>Material</span>: <strong data-bind="text: material"></strong><br>
        <span>Vendor</span>: <strong data-bind="text: vendor"></strong><br>
        <span>Weight</span>: <strong data-bind="text: weight"></strong><br>
        <!-- ko if: getActive() != -1 -->
        <span class="spool-icon" data-bind="style: { color: color_hex }" role="img" class="anticon" style="font-size: 42px; margin-right: 0px;">
            {% include 'icon_spool.svg' %}
        </span>
        <!-- /ko -->
    </div>
    <div class="modal-footer">
        <button class="btn btn-cancel" data-dismiss="modal" aria-hidden="true">Cancel</button>
        <button class="btn btn-danger btn-confirm">Start Print</button>
    </div>
</div>
